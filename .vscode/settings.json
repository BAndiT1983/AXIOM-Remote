    "cmake.configureOnOpen": false,
    "actionButtons": {
        "defaultColor": "#ff0034", // Can also use string color names.
        "loadNpmCommands": false, // Disables automatic generation of actions for npm commands.
        "commands": [
            {
                "cwd": "${workspaceFolder}/AXIOM-Remote/Firmware", // Terminal initial folder ${workspaceFolder} and os user home as defaults
                "name": "Compile",
                "color": "white",
                "singleInstance": true,
                "command": "make clean && make -j8",
            },
            {
                "name": "Flash Remote",
                "color": "white",
                "singleInstance": true,
                "command": "cd build/ && sudo pic32prog AXIOM_Remote_Firmware.hex -S",
            },
            {
                "cwd": "${workspaceFolder}/AXIOM-Remote/Firmware", // Terminal initial folder ${workspaceFolder} and os user home as defaults
                "name": "Compile & Flash",
                "color": "white",
                "singleInstance": true,
